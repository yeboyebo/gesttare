{
    "querystring": {},
    "schema": {},
    "layout": {
        "groupBoxPadre": {
            "componente": "YBGroupBox",
            "className": "gbTareas gbTareasBox ",
            "style": {},
            "layout": {
                "volverButton": {
                    "componente": "YBButton",
                    "prefix": "gt_tareas",
                    "icon": "arrow_back",
                    "className": "floatLeft",
                    "buttonType": "icon",
                    "title": "Volver",
                    "style": {
                        "marginTop": "5px",
                        "marginRight": "5px"
                    },
                    "action": {
                        "key": "acctionVolver",
                        "success": [{"slot": "return"}]
                    }
                },
                "gotoTareaButton": {
                    "componente": "YBButton",
                    "style": {
                        "padding": "0px 20px",
                        "marginTop": "5px"
                    },
                    "prefix": "gt_tareas",
                    "title": "Ir a la tarea relacionada",
                    "iconurl": "/static/dist/img/icons/clipboards.svg",
                    "className": "floatRight",
                    "buttonType": "icon",
                    "action": {
                        "key": "gotoTarea"
                    }
                },
                "separator0": {
                    "componente": "YBSeparator"
                },
                "ttForm": {
                    "prefix": "gt_timetracking",
                    "componente": "YBForm",
                    "class": "claseGrid",
                    "submit": "update",
                    "label": "Registro de tiempo",
                    "icon": "info",
                    "saveReturn": true,
                    "return": true,
                    "success": [{"slot": "return"}],
                    "fields": {
                        "gb__": {
                            "title": "",
                            "fields": {
                                "idtarea": {
                                    "function": "getTareasUsuario", 
                                    "disabled": true,
                                    "showpk": false},
                                "idusuario": {
                                    "disabled": true,
                                    "showpk": false
                                },
                                "fecha": {},
                                "horainicio": {},
                                "horafin": {},
                                "totaltiempo": {"disabled": true}
                            }
                        }
                    },
                    "exclude": {}
                }
            }
        }
    },
    "acciones": {
        "acctionVolver": {
            "action": "return"
        },
        "gotoTarea": {
            "action": "goto",
            "prefix": "gt_timetracking",
            "serverAction": "verTarea"
        }
    }
