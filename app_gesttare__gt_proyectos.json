{
    "querystring": {},
    "schema": {
        "gt_particproyecto": {
            "rel": "codproyecto",
            "querystring": {
                "p_l": 10,
                "p_c": 1,
                "o_1": "idusuario"
            }
        }
    },
    "layout": {
        "groupBoxPadre": {
            "componente": "YBGroupBox",
            "className": "gbTareas gbTareasBox",
            "style": { },
            "layout": {
                "proyectosForm": {
                    "prefix": "gt_proyectos",
                    "componente": "YBForm",
                    "class": "claseGrid",
                    "submit": "update",
                    "return": true,
                    "label": "Informaci√≥n",
                    "success": [{"slot": "return"}],
                    "fields": {
                        "gb__": {
                            "title": "",
                            "fields": {
                                "codproyecto": {"disabled": true},
                                "nombre": {},
                                "descripcion": {},
                                "estado": {},
                                "codcliente": {"combolimit": 1}
                            }
                        }
                    },
                    "exclude": {}
                },
                "groupBoxUsuarios": {
                    "componente": "YBGroupBox",
                    "className": "",
                    "layout": {
                        "participantesGrid": {
                            "label": "Participantes",
                            "icon": "group",
                            "componente": "YBGrid",
                            "prefix": "gt_particproyecto",
                            "className": "claseGrid",
                            "hideheader": true,
                            "columns": [
                                {"tipo": "field", "listpos": "title", "key": "idusuario"},
                                {
                                    "tipo": "act",
                                    "key": "delete",
                                    "label": "Borrar Linea",
                                    "success": [
                                        {"slot": "refrescar"}
                                    ]
                                }
                            ],
                            "rowclick": "",
                            "accionesGrid": [{
                                "key": "nuevaLinea",
                                "label": "Nuevo",
                                "success": []
                            }]
                        }
                    }
                }
            }
        }
    },
    "acciones": {
        "delete": {
            "action": "deleteRow",
            "icon": "delete"
        },
        "nuevaLinea": {
            "action": "newRow",
            "icon": "add_circle"
        }
    }
}
