{
    "querystring": {},
    "schema": {
        "gt_particproyecto": {
            "rel": "codproyecto",
            "querystring": {
                "p_l": 10,
                "p_c": 1,
                "o_1": "idusuario"
            }
        },
        "gt_hitosproyecto": {
            "rel": "codproyecto",
            "querystring": {
                "p_l": 10,
                "p_c": 1,
                "o_1": "fechainicio"
            }
        },
        "grafico": {
            "custom": "getRentabilidadGraphic"
        }
    },
    "layout": {
        "groupBoxPadre": {
            "componente": "YBGroupBox",
            "className": "gbTareas gbTareasBox",
            "style": { },
            "layout": {
                "volverButton": {
                    "componente": "YBButton",
                    "title": "Volver",
                    "prefix": "gt_tareas",
                    "icon": "arrow_back",
                    "className": "floatLeft",
                    "buttonType": "icon",
                    "style": {
                        "marginTop": "5px",
                        "marginRight": "5px"
                    },
                    "action": {
                        "key": "acctionVolver",
                        "success": [{"slot": "return"}]
                    }
                },
                "borrarTareaButton": {
                    "componente": "YBButton",
                    "title": "Borrar Proyecto",
                    "style": {
                        "padding": "0px 20px",
                        "marginTop": "5px"
                    },
                    "prefix": "gt_proyectos",
                    "icon": "delete",
                    "className": "floatRight",
                    "buttonType": "icon",
                    "action": {
                        "key": "borrarProyecto",
                        "success": [{"slot": "return"}]
                    }
                },
                "copiarEmailButton": {
                    "componente": "YBButton",
                    "title": "Copiar email",
                    "prefix": "gt_proyectos",
                    "icon": "email",
                    "className": "floatRight",
                    "buttonType": "icon",
                    "style": {
                        "marginTop": "5px",
                        "marginRight": "5px"
                    },
                    "action": {
                        "key": "actionEmail",
                        "success": [{"slot": "return"}]
                    }
                },
                "separator1": {
                    "componente": "YBSeparator"
                },
                "groupBoxRight": {
                "componente": "YBGroupBox",
                "className": "gbTareasLefta",
                "style": { },
                "layout": {
                    "proyectosForm": {
                        "prefix": "gt_proyectos",
                        "componente": "YBForm",
                        "icon": "info",
                        "class": "claseGrid",
                        "submit": "update",
                        "label": "Información de proyecto",
                        "fields": {
                            "gb__proyectos": {
                                "title": "",
                                "fields": {
                                    "nombre": {},
                                    "descripcion": {},
                                    "estado": {},
                                    "fechainicio": {},
                                    "fechaterminado": {},
                                    "idresponsable": {
                                        "combolimit": 1,
                                        "showpk": false,
                                        "function": "getParticCompaniaUsu"
                                    },
                                    "presupuesto": {},
                                    "idcliente": {
                                        "combolimit": 1,
                                        "showpk": false,
                                        "function": "getClientesCompaniaUsu"
                                    },
                                    "rentabilidad": {"visible": false, "disabled": true},
                                    "costetotal": {"visible": false, "disabled": true}
                                }
                            }
                        },
                        "exclude": {}
                    },
                    "separator2": {
                        "componente": "YBSeparator"
                    },
                    "nuevoParticipanteExterno": {
                        "componente": "YBButton",
                        "prefix": "gt_proyectos",
                        "label": "Añadir externo",
                        "className": "floatLeft",
                        "buttonType": "raised",
                        "action": {
                            "key": "actInvitarExterno",
                            "success": [{"slot": "refrescar"}]
                        }
                    },
                    "verTareasProyecto": {
                        "componente": "YBButton",
                        "prefix": "gt_proyectos",
                        "label": "Ver tareas",
                        "className": "floatLeft",
                        "buttonType": "raised",
                        "action": {
                            "key": "vertareas"
                        }
                    },
                    "groupBoxUsuariosC": {
                        "componente": "YBGroupBox",
                        "className": "",
                        "layout": {
                            "separator1": {
                                "componente": "YBSeparator"
                            },
                            "participantesGrid": {
                                "label": "Participantes",
                                "icon": "group",
                                "componente": "YBGrid",
                                "prefix": "gt_particproyecto",
                                "className": "claseGrid",
                                "hideheader": true,
                                "columns": [
                                    {
                                        "tipo": "act",
                                        "key": "borrarPartic",
                                        "label": "Eliminar Participante",
                                        "success": [
                                            {"slot": "refrescar"}
                                        ]
                                    },
                                    {
                                        "tipo": "foreignfield",
                                        "listpos": "title",
                                        "key": "nombre"
                                    }
                                ],
                                "rowclick": "",
                                "accionesGrid": [
                                    {
                                        "key": "actNuevoPartic",
                                        "label": "Nuevo",
                                        "success": [{"slot": "refrescar"}]
                                    }
                                ]
                            }
                        }
                    }
                }
            },
            "groupBoxRentabilidad": {
                    "componente": "YBGroupBox",
                    "className": "gbTareasRighta",
                    "style": { },
                    "label": "Análisis",
                    "visible": true,
                    "layout": {
                       "labelHorasInvertidas": {
                            "componente": "YBLabel",
                            "style": {
                                "width": "30%",
                                "border": "1px solid #dfdfdf",
                                "borderRadius": "8px",
                                "backgroundColor": "white",
                                "marginLeft": "5px",
                                "marginRight": "5px",
                                "fontSize": "48px",
                                "fontWeight": "normal",
                                "fontStyle": "normal",
                                "fonttSretch": "normal",
                                "lineHeight": "normal",
                                "letterSpacing": "normal",
                                "color": "grey",
                                "padding": "10px"
                            },
                            "titlestyle": {
                                "fontSize": "18px",
                                "letterSpacing": "1.86px",
                                "textTransform": "uppercase"
                            },
                            "title": "Horas Invertidas",
                            "text": "%yb1",
                            "action": {
                                "key": "verTrackingProyecto"
                            },
                            "params": [
                                {"type": "calc", "key": "horasinvertidas"}
                            ]
                        },
                       "labelPresupuesto": {
                            "componente": "YBLabel",
                            "style": {
                                "width": "30%",
                                "borderRadius": "8px",
                                "backgroundColor": "#bababa",
                                "marginLeft": "5px",
                                "marginRight": "5px",
                                "fontSize": "48px",
                                "fontWeight": "normal",
                                "fontStyle": "normal",
                                "fonttSretch": "normal",
                                "lineHeight": "normal",
                                "letterSpacing": "normal",
                                "color": "#ffffff",
                                "padding": "10px"
                            },
                            "titlestyle": {
                                "fontSize": "18px",
                                "letterSpacing": "1.86px",
                                "textTransform": "uppercase"
                            },
                            "title": "Presupuesto",
                            "text": "%yb1",
                            "params": [
                                {"type": "calc", "key": "presupuestoFormat"}
                            ]
                        },
                       "labelCoste": {
                            "componente": "YBLabel",
                          "style": {
                                "width": "30%",
                                "borderRadius": "8px",
                                "backgroundImage": "linear-gradient(to right, #e79b21, #ffc68d)",
                                "marginLeft": "5px",
                                "marginRight": "5px",
                                "fontSize": "48px",
                                "fontWeight": "normal",
                                "fontStyle": "normal",
                                "fonttSretch": "normal",
                                "lineHeight": "normal",
                                "letterSpacing": "normal",
                                "color": "#ffffff",
                                "padding": "10px"
                            },
                            "titlestyle": {
                                "fontSize": "18px",
                                "letterSpacing": "1.86px",
                                "textTransform": "uppercase"
                            },
                            "title": "Coste",
                            "text": "%yb1",
                            "params": [
                                {"type": "calc", "key": "costeFormat"}
                            ]
                        },
                        "separatorGraphic": {
                            "componente": "YBSeparator"
                        },
                        "groupBoxGraphic": {
                            "componente": "YBGroupBox",
                            "className": "",
                            "label": "Rentabilidad",
                            "style": {

                            },
                            "layout": {
                                "pruebagrafico": {
                                    "componente": "YBChart",
                                    "d3": "pieChart",
                                    "prefix": "grafico"
                                }
                            }
                        }
                    }
                }
                
            }
        }
    },
    "acciones": {
        "acctionVolver": {
            "action": "return"
        },
        "verTrackingProyecto": {
            "action": "goto",
            "prefix": "gt_proyectos",
            "serverAction": "verTrackingProyecto"
        },
        "actionEmail": {
            "action": "legacy",
            "serverAction": "dameEmailCreaTarea"
        },
        "borrarPartic": {
            "action": "legacy",
            "serverAction": "borrarPartic",
            "icon": "delete"
        },
        "borrarProyecto": {
            "label": "Borrar proyecto",
            "action": "legacy",
            "serverAction": "borrar_proyecto"
        },
        "delete": {
            "action": "deleteRow",
            "icon": "delete"
        },
        "vertareas": {
            "action": "goto",
            "prefix": "gt_proyectos",
            "serverAction": "vertareasproyecto"
        },
        "actNuevoPartic": {
            "label": "Nuevo participante",
            "action": "legacy",
            "prefix": "gt_proyectos",
            "icon": "group",
            "title": "Participantes",
            "serverAction": "actNuevoPartic"
        },
        "actNuevoHito": {
            "action": "legacy",
            "prefix": "gt_proyectos",
            "serverAction": "gotoNuevoHito",
            "icon": "add_circle"
        },
        "actInvitarExterno": {
            "title": "Invitación de usuarios externos",
            "action": "legacy",
            "prefix": "gt_proyectos",
            "icon": "send",
            "serverAction": "actInvitarExterno",
            "params": [
                {
                    "tipo": 3,
                    "verbose_name": "email",
                    "key": "email",
                    "validaciones": null,
                    "select": true,
                    "style": {
                        "width": "100%"
                    }
                }
            ]    
        }
    },
    "drawIf": {
        "groupBoxPadre" :{
            "groupBoxRentabilidad": "checkProyectosFormDraw"
        },
        "gb__proyectos": {
            "presupuesto": "checkProyectosFormDraw",
            "rentabilidad": "checkProyectosFormDraw",
            "costetotal": "checkProyectosFormDraw",
            "idresponsable": "checkResponsableDraw",
            "cliente": "checkResponsableDraw"
        }
    }
}
