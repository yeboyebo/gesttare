{
    "querystring": {},
    "schema": {},
    "layout": {
        "groupBoxPadre": {
            "componente": "YBGroupBox",
            "className": "gbTareas gbTareasBox",
            "style": { },
            "layout": {
                "volverButton": {
                    "componente": "YBButton",
                    "title": "Volver",
                    "prefix": "gt_tareas",
                    "icon": "arrow_back",
                    "className": "floatLeft",
                    "buttonType": "icon",
                    "style": {
                        "marginTop": "5px",
                        "marginRight": "5px"
                    },
                    "action": {
                        "key": "acctionVolver",
                        "success": [{"slot": "return"}]
                    }
                },
                "separator1": {
                    "componente": "YBSeparator"
                },
                "tareasForm": {
                    "prefix": "gt_tareas",
                    "componente": "YBForm",
                    "class": "claseGrid",
                    "submit": "create",
                    "return": false,
                    "icon": "info",
                    "label": "Nueva tarea",
                    "successmsg": "Tarea creada",
                    "saveEdit": true,
                    "saveReturn": true,
                    "fields": {
                        "gb__": {
                            "title": "",
                            "fields": {
                                "nombre": {"className": "col-sm-12"},
                                "descripcion": {},
                                "idproyecto": {"combolimit": 1, "showpk": false, "function": "getProyectosUsuario"},
                                "idhito": {
                                    "combolimit": 8, 
                                    "showpk": false, 
                                    "function": "getHitosProyecto",
                                    "params": {"idproyecto": "idproyecto"}
                                },
                                "fechavencimiento": {},
                                "fechaentrega": {},
                                "codestado": {},
                                "idusuario": {
                                    "combolimit": 1,
                                    "showpk": false,
                                    "function": "getUsuariosProyecto",
                                    "params": {"idproyecto": "idproyecto"}
                                }
                            }
                        }
                    },
                    "exclude": {}
                }
            }
        }
    },
    "acciones": {
        "acctionVolver": {
            "action": "return"
        }
    },
    "drawIf": {}
}
