{
    "querystring": {},
    "schema": {
        "mastertimetracking": {
            "query": {
                "p_l": 25
            }
        }
    },
    "layout": {
        "separator": {
            "componente": "YBSeparator"
        },
        "masterTimeTracking": {
            "componente": "YBGrid",
            "type": "query",
            "prefix": "mastertimetracking",
            "autofilter": false,
            "cansavefilter": true,
            "buscador": false,
            "filter": {
                "usuario": {
                    "label": "Usuario",
                    "filterType": "custom",
                    "tipo": 55,
                    "rel": "aqn_user",
                    "key": "idusuario",
                    "tofield": "idusuario",
                    "desc": "nombre",
                    "fN": "aqn_user.idusuario",
                    "function": "getParticProyectosUsu"
                },
                "tarea": {
                    "label": "Tarea",
                    "filterType": "custom",
                    "tipo": 55,
                    "rel": "gt_tareas",
                    "key": "idtarea",
                    "tofield": "idtarea",
                    "desc": "nombre",
                    "fN": "gt_tareas.idtarea"
                },
                "proyecto": {
                    "label": "Proyecto",
                    "filterType": "custom",
                    "tipo": 55,
                    "rel": "gt_proyectos",
                    "key": "codproyecto",
                    "tofield": "codproyecto",
                    "desc": "nombre",
                    "fN": "gt_proyectos.codproyecto",
                    "function": "getProyectosUsuario"
                },
                "fecha": {
                    "filterType": "interval"
                }
            },
            "columns": [
                {
                    "tipo": "act",
                    "key": "delete",
                    "label": "Borrar línea",
                    "success": [
                        {"slot": "refrescar"}
                    ]
                },
                {"tipo": "field", "listpos": "secondaryitem", "key": "gt_timetracking.fecha", "width": 5},
                {"tipo": "field", "listpos": "body", "key": "usuarios.nombre", "label": "Usuario", "width": 5},
                {"tipo": "field", "listpos": "title", "key": "gt_tareas.nombre", "label": "Tarea", "width": 250},
                {"tipo": "field", "listpos": "body", "key": "gt_proyectos.nombre", "label": "Proyecto", "width": 150},
                {"tipo": "field", "listpos": "subtitle", "key": "gt_timetracking.totaltiempo", "width": 5}
            ],
            "rowclick": "gototimetracking",
            "accionesGrid": [{
                "key": "getReport",
                "label": "Genera XLS",
                "success": []
            }]
        }
    },
    "acciones": {
        "getReport": {
            "label" : "",
            "action": "onexport",
            "icon": "save"
        },
        "gototimetracking": {
            "action": "goto",
            "template": "form"
        },
        "editarTT": {
            "action": "legacy",
            "prefix": "gt_timetracking",
            "serverAction": "editartt",
            "icon": "edit"
        },
        "delete": {
            "action": "deleteRow",
            "prefix": "gt_timetracking",
            "icon": "delete",
            "msg": "El registro de tiempo será eliminado"
        }
    }
}
