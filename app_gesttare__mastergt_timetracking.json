{
    "querystring": {},
    "schema": {
        "mastertimetracking": {
            "query": {
                "p_l": 25
            }
        },
        "mastertimetrackingagrupado": {
            "query": {
                "p_l": 25
            }
        }
    },
    "layout": {
        "botonera": {
            "componente": "YBGroupBox",
            "className": "actionGB",
            "style": {"zIndex": 3, "position": "relative"},
            "layout": {
                "vistaNoAgrupada": {
                    "componente": "YBButton",
                    "prefix": "gt_timetracking",
                    "label": "NO AGRUPAR POR TAREA",
                    "className": "floatRight agruparTarea",
                    "buttonType": "raised",
                    "action": {
                        "key": "vistaNoAgrupada",
                        "success": [{"slot": "recargar"}]
                    }
                },
                "vistaAgrupada": {
                    "componente": "YBButton",
                    "prefix": "gt_timetracking",
                    "label": "AGRUPAR POR TAREA",
                    "className": "floatRight agruparTarea",
                    "buttonType": "raised",
                    "action": {
                        "key": "vistaAgrupada",
                        "success": [{"slot": "recargar"}]
                    }
                }
            }
        },
        "groupBoxPadre": {
            "componente": "YBGroupBox",
            "className": " ",
            "style": {},
            "layout": {
                "separator": {
                    "componente": "YBSeparator"
                },
                "masterTimeTracking": {
                    "componente": "YBGrid",
                    "type": "query",
                    "prefix": "mastertimetracking",
                    "autofilter": false,
                    "cansavefilter": true,
                    "className": "",
                    "buscador": false,
                    "filter": {
                        "usuario": {
                            "label": "Usuario",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "aqn_user",
                            "key": "idusuario",
                            "tofield": "idusuario",
                            "desc": "usuario",
                            "fN": "aqn_user.idusuario",
                            "showpk": false,
                            "function": "getParticProyectosUsu"
                        },
                        "tarea": {
                            "label": "Tarea",
                            "filterType": "custom",
                            "tipo": 3,
                            "key": "tarea"
                        },
                        "proyecto": {
                            "label": "Proyecto",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "gt_proyectos",
                            "key": "idproyecto",
                            "tofield": "idproyecto",
                            "desc": "nombre",
                            "fN": "gt_proyectos.idproyecto",
                            "showpk": false,
                            "function": "getProyectosUsuario"
                        },
                        "hito": {
                            "label": "Hito",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "gt_hitosproyecto",
                            "key": "idhito",
                            "tofield": "idhito",
                            "desc": "nombre",
                            "fN": "gt_hitosproyecto.idhito",
                            "showpk": false,
                            "function": "getHitosProyectosUsu"
                        },
                        "cliente": {
                            "label": "Cliente",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "gt_clientes",
                            "key": "idcliente",
                            "tofield": "idcliente",
                            "desc": "nombre",
                            "fN": "gt_clientes.idcliente",
                            "showpk": false,
                            "function": "getClientesCompaniaUsu"
                        },
                        "fecha": {
                            "filterType": "interval"
                        }
                    },
                    "columns": [
                        {
                            "tipo": "act",
                            "key": "delete",
                            "label": "Eliminar Tracking",
                            "success": [
                                {"slot": "refrescar"}
                            ]
                        },
                        {"tipo": "field", "listpos": "secondaryitem", "key": "gt_timetracking.fecha", "width": 5},
                        {"tipo": "field", "listpos": "body", "key": "aqn_user.usuario","color": "Color usuario", "label": " ", "width": 20},
                        {"tipo": "field", "listpos": "title", "key": "gt_tareas.nombre", "label": "Tarea", "title": true,"width": 370},
                        {"tipo": "foreignfield", "listpos": "body", "key": "Proyecto", "label": "Proyecto", "title": true,"width": 180},
                        {"tipo": "field", "listpos": "title", "key": "gt_hitosproyecto.nombre", "label": "Hito", "title": true, "visible": false,"width": 370},
                        {"tipo": "foreignfield", "listpos": "body", "key": "Cliente", "label": "Cliente", "title": true, "visible": false,"width": 180},
                        {"tipo": "field", "listpos": "subtitle", "key": "gt_timetracking.totaltiempo", "width": 5}
                    ],
                    "rowclick": "gototimetracking",
                    "accionesGrid": [{
                        "key": "getReport",
                        "label": "Genera XLS",
                        "success": []
                    }]
                },
                "masterTimeTrackingAgrupado": {
                    "componente": "YBGrid",
                    "type": "query",
                    "prefix": "mastertimetracking",
                    "autofilter": false,
                    "cansavefilter": true,
                    "className": "",
                    "buscador": false,
                    "paginacion": false,
                    "filter": {
                        "usuario": {
                            "label": "Usuario",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "aqn_user",
                            "key": "idusuario",
                            "tofield": "idusuario",
                            "desc": "usuario",
                            "fN": "aqn_user.idusuario",
                            "showpk": false,
                            "function": "getParticProyectosUsu"
                        },
                        "tarea": {
                            "label": "Tarea",
                            "filterType": "custom",
                            "tipo": 3,
                            "key": "tarea"
                        },
                        "proyecto": {
                            "label": "Proyecto",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "gt_proyectos",
                            "key": "idproyecto",
                            "tofield": "idproyecto",
                            "desc": "nombre",
                            "fN": "gt_proyectos.idproyecto",
                            "showpk": false,
                            "function": "getProyectosUsuario"
                        },
                        "hito": {
                            "label": "Hito",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "gt_hitosproyecto",
                            "key": "idhito",
                            "tofield": "idhito",
                            "desc": "nombre",
                            "fN": "gt_hitosproyecto.idhito",
                            "showpk": false,
                            "function": "getHitosProyectosUsu"
                        },
                        "cliente": {
                            "label": "Cliente",
                            "filterType": "custom",
                            "tipo": 55,
                            "rel": "gt_clientes",
                            "key": "idcliente",
                            "tofield": "idcliente",
                            "desc": "nombre",
                            "fN": "gt_clientes.idcliente",
                            "showpk": false,
                            "function": "getClientesCompaniaUsu"
                        },
                        "fecha": {
                            "filterType": "interval"
                        }
                    },
                    "columns": [
                        {"tipo": "foreignfield", "listpos": "title", "key": "gt_tareas.nombre", "label": "Tarea", "title": true,"width": 570},
                        {"tipo": "foreignfield", "listpos": "body", "key": "Proyecto", "label": "Proyecto", "title": true,"width": 180},
                        {"tipo": "foreignfield", "listpos": "subtitle", "key": "suma","label": "Total tiempo", "width": 5}
                    ],
                    "rowclick": "gototarea"
                }

            }
        }
    },
    "acciones": {
        "getReport": {
            "label" : "",
            "action": "onexport",
            "icon": "cloud_download"
        },
        "gototimetracking": {
            "action": "goto",
            "template": "form"
        },
        "gototarea": {
            "action": "goto",
            "prefix": "gt_tareas",
            "serverAction": "verTarea",
            "icon": "assignment"
        },
        "editarTT": {
            "action": "legacy",
            "prefix": "gt_timetracking",
            "serverAction": "editartt",
            "icon": "edit"
        },
        "delete": {
            "action": "deleteRow",
            "prefix": "gt_timetracking",
            "icon": "delete",
            "msg": "El registro de tiempo será eliminado"
        },
        "vistaNoAgrupada": {
            "action": "legacy",
            "serverAction": "set_estado_noagrupado",
            "icon": "icon"
        },
        "vistaAgrupada": {
            "action": "legacy",
            "serverAction": "set_estado_agrupado",
            "icon": "icon"
        }
    },
    "drawIf": {
        "groupBoxPadre": {
            "masterTimeTracking": "drawif_noagrupado",
            "masterTimeTrackingAgrupado": "drawif_agrupado"
        },
        "botonera": {
            "vistaNoAgrupada": "drawif_botonnoagrupado",
            "vistaAgrupada": "drawif_botonagrupado"
        }
    }
}
